{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "book"
    ],
    "properties": {
        "book": {
            "type": "object",
            "default": {},
            "title": "The book Schema",
            "required": [
                "isbn",
                "amazon_url",
                "author",
                "language",
                "pages",
                "publisher",
                "title",
                "year"
            ],
            "properties": {
                "isbn": {
                    "type": "string",
                    "default": "",
                    "title": "The isbn Schema",
                    "examples": [
                        "4242"
                    ]
                },
                "amazon_url": {
                    "type": "string",
                    "default": "",
                    "title": "The amazon_url Schema",
                    "examples": [
                        "wwww.amazon.com"
                    ]
                },
                "author": {
                    "type": "string",
                    "default": "",
                    "title": "The author Schema",
                    "examples": [
                        "JK Rowling"
                    ]
                },
                "language": {
                    "type": "string",
                    "default": "",
                    "title": "The language Schema",
                    "examples": [
                        "english"
                    ]
                },
                "pages": {
                    "type": "integer",
                    "default": 0,
                    "title": "The pages Schema",
                    "examples": [
                        456
                    ]
                },
                "publisher": {
                    "type": "string",
                    "default": "",
                    "title": "The publisher Schema",
                    "examples": [
                        "penguinhouse"
                    ]
                },
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "to be or not to be"
                    ]
                },
                "year": {
                    "type": "integer",
                    "default": 0,
                    "title": "The year Schema",
                    "examples": [
                        1965
                    ]
                }
            },
            "examples": [{
                "isbn": "4242",
                "amazon_url": "wwww.amazon.com",
                "author": "JK Rowling",
                "language": "english",
                "pages": 456,
                "publisher": "penguinhouse",
                "title": "to be or not to be",
                "year": 1965
            }]
        }
    },
    "examples": [{
        "book": {
            "isbn": "4242",
            "amazon_url": "wwww.amazon.com",
            "author": "JK Rowling",
            "language": "english",
            "pages": 456,
            "publisher": "penguinhouse",
            "title": "to be or not to be",
            "year": 1965
        }
    }]
}